{
    "automock": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
        "./src/components/Errors/*.{js,jsx,ts,tsx}",
        "./src/components/Items/Hidden/*.{js,jsx,ts,tsx}",
        "./src/components/Items/Subtypes/{Radio,Text}/*.{js,jsx,ts,tsx}",
        "!./src/components/Items/Hidden/*Edit.{js,jsx,ts,tsx}",
        "!./src/components/Items/Subtypes/{Radio,Text}/*Edit.{js,jsx,ts,tsx}",
        "!./src/**/index.{js,jsx,ts,tsx}",
        "!./src/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
        "global": {
            "statements": 10,
            "branches": 10,
            "functions": 10,
            "lines": 10
        }
    },
    "moduleNameMapper": {
        "\\.(css|sass|scss|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
        "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jsdom"
}