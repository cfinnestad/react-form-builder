{
    "automock": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
        "./src/components/Errors/*.{js,jsx,ts,tsx}",
        "./src/components/Items/Subtypes/{Radio,Text}/*.{js,jsx,ts,tsx}",
        "!./src/**/index.{js,jsx,ts,tsx}",
        "!./src/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
        "global": {
            "statements": 10,
            "branches": 10,
            "functions": 10,
            "lines": 10
        }
    },
    "testEnvironment": "jsdom"
}